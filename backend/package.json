{
	"name": "backend",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"types:db": "kysely-codegen --out-file src/database/schema.ts --dialect postgres",
		"types:gql": "tsx src/schema.ts",
		"schema": "pnpm types:db && pnpm types:gql",
		"server": "tsx watch --include src src/server.ts",
		"test": "tsx --test test/**/*.ts"
	},
	"devDependencies": {
		"@types/graphql": "^14.5.0",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/ms": "^0.7.34",
		"@types/node": "22.8.6",
		"@types/pg": "^8.11.10",
		"@types/ws": "8.5.12",
		"ansicolor": "^2.0.3",
		"kysely-codegen": "^0.17.0"
	},
	"dependencies": {
		"@graphql-tools/schema": "^10.0.7",
		"graphql-fields": "^2.0.3",
		"graphql-scalars": "^1.23.0",
		"graphql-ws": "^5.16.0",
		"graphql-yoga": "^5.8.0",
		"graphql": "^16.9.0",
		"jsonwebtoken": "^9.0.2",
		"kysely": "^0.27.4",
		"ms": "2.1.3",
		"pg": "^8.13.1",
		"reflect-metadata": "^0.2.2",
		"type-graphql": "2.0.0-rc.2",
		"inversify": "6.0.3",
		"ws": "8.18.0"
	}
}
