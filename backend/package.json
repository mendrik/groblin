{
	"name": "backend",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"types:db": "kysely-codegen --out-file src/database/schema.ts --dialect postgres",
		"types:gql": "tsx src/schema.ts",
		"schema:db": "./dump_db.sh",
