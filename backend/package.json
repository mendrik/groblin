		"@types/ws": "8.5.13",
		"ansicolor": "^2.0.3",
		"kysely-codegen": "^0.17.0",
		"vite-tsconfig-paths": "^5.1.5",
		"vitest": "^2.1.8"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.705.0",
		"@aws-sdk/s3-request-presigner": "^3.705.0",
		"@graphql-tools/schema": "^10.0.11",
		"@types/chroma-js": "^2.4.4",
		"chroma-js": "^3.1.2",
		"graphql": "^16.9.0",
		"graphql-fields": "^2.0.3",
		"graphql-scalars": "^1.24.0",
		"graphql-ws": "^5.16.0",
