		"jsonwebtoken": "^9.0.2",
		"kysely": "^0.27.5",
		"ms": "2.1.3",
		"pg": "^8.13.1",
		"reflect-metadata": "^0.2.2",
		"type-graphql": "2.0.0-rc.2",
		"uuid": "11.0.3",
		"ws": "8.18.0",
		"zod": "^3.23.8"
	}
